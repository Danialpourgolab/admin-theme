{
  "name": "admin-theme/admin-theme",
  "description": "Lightweight Wordpress Dashboard Theme",
  "minimum-stability": "stable",
  "license": "GPL2",
  "authors": [
    {
      "name": "Danial Pourgolab",
      "email": "dp.abzarwp@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-zlib": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.33"
    }
  },
  "autoload": {
    "psr-4": {
      "AdminTheme\\": "src",
      "AdminTheme\\Includes\\": "includes",
      "AdminTheme\\SDK\\": "sdk"
    }
  }
}